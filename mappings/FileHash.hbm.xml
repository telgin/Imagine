<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="FileHash" table="file_hash">
		<meta attribute="class-description">
			This is the table of file hashes per tracking group. These represent files that were written.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="native"/>
		</id>
		<property name="groupid" column="groupid" type="int"/>
		<property name="f1uuid" column="f1uuid" type="int"/>
		<property name="hash" column="hash" type="byte[]"/>
	</class>
</hibernate-mapping>